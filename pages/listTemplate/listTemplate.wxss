/* pages/listTemplate/listTemplate.wxss */

.temContainer {
    display: flex;
    flex-direction: column;
}

.avatar_date image {
    width: 64rpx;
    height: 64rpx;
    vertical-align: middle;
    padding: 15rpx;
}
.avatar_date text {
    font-size: 30rpx;
    vertical-align: middle;
}

.caption {
    font-size: 35rpx;
    font-weight: bold;
    margin-bottom: 10rpx;
}

.tepImg {
    width: 100%;
    height: 400rpx;
}

.details {
    font-size: 35rpx;
    text-indent: 1em;
}

.star_view image {
    width: 32rpx;
    height: 32rpx;
    vertical-align: middle;
    margin-left: 15rpx;
}
.star_view text {
    font-size: 25rpx;
    vertical-align: middle;
    margin-left: 15rpx;
}